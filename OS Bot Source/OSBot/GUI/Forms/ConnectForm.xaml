<Window x:Class="OSBot.GUI.Forms.ConnectForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Connect" Height="192" Width="317" ResizeMode="NoResize" WindowStartupLocation="CenterOwner">
    <Grid>
        <Label Content="ID/E-Mail:" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="label1" VerticalAlignment="Top" Width="74" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="12,37,0,0" Name="TextBox_ID" VerticalAlignment="Top" Width="271" />
        <Label Content="Password:" Height="28" HorizontalAlignment="Left" Margin="12,66,0,0" Name="label2" VerticalAlignment="Top" Width="74" />
        <PasswordBox Height="23" HorizontalAlignment="Left" Margin="12,91,0,0" Name="PasswordBox_Password" VerticalAlignment="Top" Width="271" />
        <Button Content="Connect" Height="23" HorizontalAlignment="Left" Margin="208,120,0,0" Name="Button_Connect" VerticalAlignment="Top" Width="75" />
        <Label Height="28" HorizontalAlignment="Left" Margin="12,119,0,0" Name="Label_Status" VerticalAlignment="Top" Width="190" />
    </Grid>
</Window>
