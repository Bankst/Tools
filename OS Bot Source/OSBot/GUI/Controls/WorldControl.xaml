                <Label Content="Name:" Height="28" HorizontalAlignment="Left" Margin="6,61,0,0" Name="label2" VerticalAlignment="Top" Width="58" />
                <Label Content="Status:" Height="28" HorizontalAlignment="Left" Margin="6,87,0,0" Name="label3" VerticalAlignment="Top" Width="58" />
                <Label Content="-" Height="28" HorizontalAlignment="Left" Margin="70,35,0,0" Name="Label_WorldSelect_WorldID" VerticalAlignment="Top" Width="120" HorizontalContentAlignment="Center" />
                <Label Content="-" Height="28" HorizontalAlignment="Left" HorizontalContentAlignment="Center" Margin="70,61,0,0" Name="Label_WorldSelect_WorldName" VerticalAlignment="Top" Width="120" />
                <Label Content="-" Height="28" HorizontalAlignment="Left" HorizontalContentAlignment="Center" Margin="70,87,0,0" Name="Label_WorldSelect_WorldStatus" VerticalAlignment="Top" Width="120" />
                <Button Content="Connect" Height="23" HorizontalAlignment="Left" Margin="6,121,0,0" Name="Button_WorldSelect_ConnectToggle" VerticalAlignment="Top" Width="184" />
            </Grid>
        </GroupBox>
    </Grid>
</UserControl>
